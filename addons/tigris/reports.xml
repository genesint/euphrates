<openerp>
    <data>

        <menuitem action="ir_export_extended_ept.ir_exports_action_ept"
                  id="menu_action_export" parent="base.menu_sales" name="Export Reports"
                  sequence="12"/>

        <record id="pos_report_view" model="ir.ui.view">
            <field name="name">POS Report View</field>
            <field name="model">pos.report.model</field>
            <field name="arch" type="xml">
                <tree string="POS Report" create="false">
                    <field name="id" invisible="1"/>
                    <field name="create_date"/>
                    <field name="login"/>
                    <field name="pos_reference"/>
                    <field name="name_template"/>
                    <field name="complete_name"/>
                    <field name="sale_price_unit"/>
                    <field name="cost_price_unit"/>
                    <field name="price_subtotal"/>
                    <field name="qty"/>
                    <field name="profit"/>
                </tree>
            </field>
        </record>

        <record id="view_pos_report_filter" model="ir.ui.view">
            <field name="name">pos.report.search</field>
            <field name="model">pos.report.model</field>
            <field name="arch" type="xml">
                <search string="Search POS Report">
                    <field name="login" />
                    <field name="pos_reference"/>
                    <field name="name_template"/>
                    <field name="complete_name"/>
                </search>
            </field>
        </record>
        <record id="action_pos_report" model="ir.actions.act_window">
            <field name="name">POS Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">pos.report.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="pos_report_view"/>
            <field name="search_view_id" ref="view_pos_report_filter"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    The POS Report
                </p>

            </field>
        </record>
        <menuitem action="action_pos_report"
                  id="menu_action_pos_report" parent="base.menu_sales"
                  sequence="12"/>

        <record id="stock_quant_view" model="ir.ui.view">
            <field name="name">Stock Quant View</field>
            <field name="model">stock.quant.model</field>
            <field name="arch" type="xml">
                <tree string="Stock Report" create="false">
                    <field name="id" invisible="1"/>
                    <field name="create_date"/>
                    <field name="write_date"/>
                    <field name="in_date"/>
                    <field name="name_template"/>
                    <field name="current_location"/>
                    <field name="qty"/>
                    <field name="cost"/>
                    <field name="inventory_value"/>
                </tree>
            </field>
        </record>

        <record id="view_stock_quant_filter" model="ir.ui.view">
            <field name="name">stock.quant.search</field>
            <field name="model">stock.quant.model</field>
            <field name="arch" type="xml">
                <search string="Search Stock Quant Report">
                    <field name="name_template"/>
                    <field name="current_location"/>
                </search>
            </field>
        </record>

        <record id="action_stock_quant_report" model="ir.actions.act_window">
            <field name="name">Stock Quant Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.quant.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="stock_quant_view"/>
            <field name="search_view_id" ref="view_stock_quant_filter"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    The Stock Quant Report
                </p>
            </field>
        </record>
        <menuitem action="action_stock_quant_report"
                  id="menu_action_stock_quant_report" parent="base.menu_sales"
                  sequence="12"/>

        <record id="stock_move_view" model="ir.ui.view">
            <field name="name">Stock Move View</field>
            <field name="model">stock.move.model</field>
            <field name="arch" type="xml">
                <tree string="Stock Move Report" create="false">
                    <field name="id" invisible="1"/>
                    <field name="create_date"/>
                    <field name="date"/>
                    <field name="origin"/>
                    <field name="name"/>
                    <field name="product"/>
                    <field name="location1"/>
                    <field name="location2"/>
                    <field name="product_qty"/>
                    <field name="price_unit"/>
                    <field name="inventory_value"/>
                </tree>
            </field>
        </record>

        <record id="view_stock_move_filter" model="ir.ui.view">
            <field name="name">stock.move.search</field>
            <field name="model">stock.move.model</field>
            <field name="arch" type="xml">
                <search string="Search Stock Move Report">
                    <field name="name"/>
                    <field name="origin"/>
                    <field name="product"/>
                    <field name="location1"/>
                    <field name="location2"/>
                </search>
            </field>
        </record>

        <record id="action_stock_move_report" model="ir.actions.act_window">
            <field name="name">Stock Move Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.move.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="stock_move_view"/>
            <field name="search_view_id" ref="view_stock_move_filter"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    The Stock Move Report
                </p>
            </field>
        </record>
        <menuitem action="action_stock_move_report"
                  id="menu_action_stock_move_report" parent="base.menu_sales"
                  sequence="12"/>
    </data>
</openerp>